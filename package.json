{
  "name": "stopwatch",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm install && cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm install && cross-env NODE_ENV=production webpack --optimize-minimize"
  },
  "dependencies": {
    "babel-core": "~6.24.1",
    "babel-loader": "~7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "~5.0.1",
    "jquery": "~3.2.1",
    "mobx": "^3.1.16",
    "mobx-react": "^4.2.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "uglifyjs-webpack-plugin": "~0.4.3",
    "webpack": "~2.6.1",
    "webpack-dev-server": "~2.4.5"
  },
  "babel": {
    "presets": [
      "stage-1",
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  }
}
